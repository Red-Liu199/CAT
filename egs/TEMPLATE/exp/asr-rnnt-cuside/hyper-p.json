{
    "data": {
        "train": "yesno",
        "dev": "yesno",
        "test": "yesno",
        "filter": "10:2000",
        "lang": "en"
    },
    "tokenizer": {
        "type": "SentencePieceTokenizer",
        "property": {
            "model_type": "char",
            "use_all_vocab": true,
            "bos_id": 0,
            "unk_id": 1,
            "model_prefix": "sentencepiece/yesno_char/spm",
            "vocab_size": 8
        }
    },
    "train": {
        "bin": "cat.rnnt.train_unified",
        "option": {
            "amp": true,
            "batch_size": 4,
            "check_freq": 50
        }
    },
    "inference": {
        "infer": {
            "bin": "cat.rnnt.decode",
            "option": {
                "unified": true,
                "streaming": true,
                "beam_size": 16,
                "cpu": true,
                "nj": 40
            }
        },
        "er": {
            "cer": false
        }
    }
}