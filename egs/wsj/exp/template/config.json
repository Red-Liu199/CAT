{
    "specaug_config": {
        "freq_mask_width_range": 0.34,
        "num_freq_mask": 2,
        "num_time_mask": 2,
        "time_mask_width_range": 0.1,
        "time_warp_window": 0.2
    },
    "transducer": {
        "compact": true,
        "decoder_mask_range": 0.2,
        "fused": true,
        "num_decoder_mask": 4
    },
    "joint": {
        "type": "JointNet",
        "kwargs": {
            "hdim": 128,
            "num_classes": 1024,
            "odim_decoder": 128,
            "odim_encoder": 128
        }
    },
    "decoder": {
        "type": "LSTMPredictNet",
        "kwargs": {
            "classical": false,
            "hdim": 128,
            "num_classes": 1024,
            "num_layers": 1,
            "variational_noise": [
                0.0,
                0.075
            ]
        }
    },
    "encoder": {
        "type": "ConformerNet",
        "kwargs": {
            "conv": "vgg2l",
            "d_head": -1,
            "dropout": 0.3,
            "dropout_attn": 0.3,
            "hdim": 128,
            "idim": 80,
            "kernel_size": 15,
            "num_cells": 6,
            "num_classes": 1024,
            "num_heads": 4
        }
    },
    "scheduler": {
        "type": "SchedulerEarlyStop",
        "kwargs": {
            "epoch_min": 10,
            "gamma": 0.1,
            "lr_stop": 1e-06,
            "reverse_metric_direc": true
        },
        "optimizer": {
            "type": "Adam",
            "use_zero": true,
            "kwargs": {
                "lr": 0.001
            }
        }
    }
}
