{
    "specaug": {
        "freq_mask_width_range": 0.34,
        "num_freq_mask": 2,
        "num_time_mask": 5,
        "time_mask_width_range": 0.1,
        "time_warp_window": 0.2
    },
    "transducer": {
        "compact": true,
        "predictor_mask_range": 0.2,
        "num_predictor_mask": 4
    },
    "encoder": {
        "type": "ConformerNet",
        "kwargs": {
            "num_cells": 12,
            "idim": 80,
            "hdim": 512,
            "conv": "vgg2l",
            "num_heads": 4,
            "dropout": 0.3,
            "dropout_attn": 0.3,
            "kernel_size": 15,
            "num_classes": 4232,
            "with_head": false,
            "time_reduction_factor": 2
        }
    },
    "decoder": {
        "type": "SyllableEnhancedLSTM",
        "kwargs": {
            "syllable_data": "exp/test-phn-cn/char2syllable.pkl",
            "num_classes": 4232,
            "hdim": 512,
            "num_layers": 1,
            "with_head": false,
            "variational_noise": [
                0.0,
                0.075
            ]
        }
    },
    "joiner": {
        "type": "JointNet",
        "kwargs": {
            "odim_enc": 512,
            "odim_pred": 512,
            "hdim": 512,
            "num_classes": 4232,
            "compact": true
        }
    },
    "scheduler": {
        "type": "SchedulerNoam",
        "optimizer": {
            "type": "Adam",
            "zeroredundancy": true,
            "kwargs": {
                "lr": 0.0003,
                "betas": [
                    0.9,
                    0.98
                ],
                "weight_decay": 1e-06
            }
        },
        "kwargs": {
            "dim_model": 512,
            "warmup_step": 25000,
            "stop_step": 80000,
            "peak_factor": 1.0
        }
    }
}