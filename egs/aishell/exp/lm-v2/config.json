{
    "decoder": {
        "type": "LSTMPredictNet",
        "kwargs": {
            "num_classes": 4232,
            "hdim": 1024,
            "num_layers": 2,
            "dropout": 0.5,
            "variational_noise": [
                0.0,
                0.075
            ]
        }
    },
    "scheduler": {
        "type": "SchedulerTransformer",
        "optimizer": {
            "type": "Adam",
            "use_zero": true,
            "kwargs": {
                "lr": 0.001,
                "betas": [
                    0.9,
                    0.98
                ],
                "weight_decay": 1e-06
            }
        },
        "kwargs": {
            "d_model": 256,
            "epoch_max": 50,
            "peak_factor": 0.5,
            "warmup_steps": 4000,
            "reverse_metric_direc": true
        }
    }
}